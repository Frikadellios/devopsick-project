{
  "name": "devopsick-project",
  "type": "module",
  "version": "1.0.1",
  "repository": "https://github.com/Frikadellios/devopsick-project.git",
  "author": "Hrihorii Ilin <devopsick@pm.me>",
  "scripts": {
    "editorconfig": "editorconfig-checker -exclude \"**/node_modules/**\"",
    "dev": "bunx --bun astro dev --verbose",
    "start": "bun wrangler types && bun astro check --verbose && bunx --bun astro dev --verbose",
    "build": "bun wrangler types && bun astro check --verbose && bunx --bun astro build --verbose",
    "preview": "bunx --bun astro build && bun wrangler pages dev ./dist",
    "astro": "astro",
    "deploy": "bunx --bun astro build && bun wrangler pages deploy dist",
    "browserslist:update": "bunx update-browserslist-db@latest",
    "cf-typegen": "wrangler types",
    "format": "biome format . --write",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --apply",
    "lint:fix:unsafe": "biome lint . --apply-unsafe",
    "biomes": "bunx @biomejs/biome check --write .",
    "biomes:fix": "bunx @biomejs/biome check --fix --unsafe .",
    "tailwind:cli": "bunx @tailwindcss/cli@next -i ./src/styles/app.css -o ./src/styles/dist/app.css"
  },
  "engines": {
    "node": ">=18.19.1 <19 || >=20.14.0 <21 || >=21.1.0"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/bun": "latest",
    "@types/html": "^1.0.4",
    "@types/node": "^20.14.11",
    "astro-zod-to-json-schema": "^0.0.2",
    "biome-config-astro": "^1.2.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.9",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "terser": "^5.31.3",
    "unplugin-auto-import": "^0.18.0",
    "vite-plugin-lightningcss": "^0.0.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@astrojs/check": "^0.8.2",
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/partytown": "^2.1.1",
    "@astrojs/react": "^3.6.0",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/ts-plugin": "^1.9.0",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.0",
    "@nanostores/persistent": "^0.10.1",
    "@nanostores/react": "^0.7.2",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@tailwindcss/cli": "^4.0.0-alpha.17",
    "@tailwindcss/postcss": "^4.0.0-alpha.17",
    "@tailwindcss/vite": "^4.0.0-alpha.17",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-legacy": "^5.4.1",
    "astro": "^4.12.2",
    "astro-icon": "^1.1.0",
    "astro-nanointl": "^1.0.0",
    "balance-text": "^3.3.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "detect-browser-es": "^0.1.0",
    "embla-carousel-react": "^8.1.7",
    "framer-motion": "^11.3.8",
    "lucide-react": "^0.408.0",
    "mdast-util-to-string": "^4.0.0",
    "nanostores": "^0.10.3",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-use-signals": "^1.7.7",
    "reading-time": "^1.5.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^4.0.0-alpha.17",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "zod": "^3.23.8"
  },
  "trustedDependencies": ["@biomejs/biome"]
}
